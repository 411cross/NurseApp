<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.whatever.nurseapp.nurseapp.activity.MainActivity">

    <Button
        android:id="@+id/new_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="新订单"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="231dp"
        android:layout_alignBaseline="@+id/old_order"
        android:layout_alignBottom="@+id/old_order"
        android:layout_toLeftOf="@+id/old_order"
        android:layout_toStartOf="@+id/old_order"
        android:layout_marginRight="55dp"
        android:layout_marginEnd="55dp" />

    <Button
        android:id="@+id/old_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="旧订单"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="231dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="66dp"
        android:layout_marginEnd="66dp" />
</RelativeLayout>
