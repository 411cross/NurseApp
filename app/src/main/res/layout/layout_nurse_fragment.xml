<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/nurse_list_btn"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="73dp"
        android:layout_weight="1"
        app:srcCompat="@mipmap/look_through_nurse" />

    <ImageView
        android:id="@+id/add_nurse_btn"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignStart="@+id/nurse_list_btn"
        android:layout_below="@+id/nurse_list_btn"
        android:layout_marginTop="11dp"
        android:layout_weight="1"
        app:srcCompat="@mipmap/add_nurse" />

</RelativeLayout>
